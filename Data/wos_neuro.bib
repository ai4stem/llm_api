@article{ WOS:000071268900003,
Author = {Glick, TH and Armstrong, EG and Waterman, MA and Hundert, EM and Hyman,
   SE},
Title = {An integrated preclerkship curriculum in neuroscience, psychiatry, and
   neurology},
Journal = {ACADEMIC PSYCHIATRY},
