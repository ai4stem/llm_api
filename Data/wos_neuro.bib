@article{ WOS:000560149000005,
Author = {Shaywitz, Bennett A. and Shaywitz, Sally E.},
Title = {The American experience: towards a 21st century definition of dyslexia},
Journal = {OXFORD REVIEW OF EDUCATION},
Year = {2020},
Volume = {46},
Number = {4, SI},
Pages = {454-471},
Month = {JUL 3},
Abstract = {We review the evolution of the conceptualisation of dyslexia and along
   with it the current, 21st century definition of dyslexia. Our starting
   point is the seminal report by Pringle Morgan in 1896, followed by early
   20th century reports by Hinshelwood, and continuing with concepts of
   brain injury and minimal brain dysfunctions then to the emergence of
   `learning disability' in the mid-20th century. Within this historical
   framework we note the emergence and scientific validation of an
   `unexpected' model culminating in the first definition of dyslexia in
   U.S. federal law in late 2018 and in recent U.S. federal civil rights
   law. We next address the issue of whether dyslexia is `real' by showing
   how advances in non-invasive functional brain imaging have led to a
   replicated, robust neural signature for dyslexia. We close by
   illustrating how science has informed policy, emphasising the focus on
   evidence-based practices beginning with the report of the National
   Reading Panel and including the creation of the What Works
   Clearinghouse. Early screening for dyslexia represents another important
   policy accomplishment with dyslexia laws now passed in over 30 states.
   Evidence-based effective interventions for dyslexia, including free,
   public charter schools specialised for dyslexic children exemplify the
   most recent policy initiative.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Shaywitz, BA (Corresponding Author), Yale Univ, Dept Pediat, Yale Ctr Dyslexia \& Creat, Sch Med, New Haven, CT 06511 USA.
   Shaywitz, Bennett A.; Shaywitz, Sally E., Yale Univ, Dept Pediat, Yale Ctr Dyslexia \& Creat, Sch Med, New Haven, CT 06511 USA.},
DOI = {10.1080/03054985.2020.1793545},
