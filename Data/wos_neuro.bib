@article{ WOS:000886165200004,
Author = {Gentile, Amber and Oswald, A. Michele},
Title = {The Oswald-Gentile Model of Instruction: A Holistic Approach},
Journal = {INTERNATIONAL JOURNAL OF TECHNOLOGY IN EDUCATION},
Year = {2021},
