@article{ WOS:000222996400007,
Author = {Stern, E},
Title = {How much brain does the school need? Prospects and limits of
   neuro-psychological research on teaching and learning},
Journal = {ZEITSCHRIFT FUR PADAGOGIK},
Year = {2004},
Volume = {50},
Number = {4},
Pages = {531-538},
Month = {JUL-AUG},
Abstract = {In spite of its fascinating findings on human information processing,
   brain research as yet only allows for some very general conclusions as
   to the layout of learning opportunities in pre-school and school.
   Educational institutions have been established to enable students to
   acquire and apply, within a few years, knowledge which it has taken
   mankind hundreds or thousands of years to elaborate. For this to be
   successful, teachers have to acquire pedagogical content knowledge and
   use it to optimise learning opportunities. The history of science as
   well as cognitive psychology provide insights that may help teachers to
   build up adequate pedagogical content knowledge.},
