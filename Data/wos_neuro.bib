@article{ WOS:000479625100001,
Author = {Jones, Stephanie M. and McGarrah, Michael W. and Kahn, Jennifer},
Title = {Social and Emotional Learning: A Principled Science of Human Development
   in Context},
Journal = {EDUCATIONAL PSYCHOLOGIST},
Year = {2019},
Volume = {54},
