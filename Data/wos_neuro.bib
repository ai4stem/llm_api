@article{ WOS:000207714700002,
Author = {Lee, Carol D.},
Title = {2008 Wallace Foundation Distinguished Lecture The Centrality of Culture
   to the Scientific Study of Learning and Development: How an Ecological
   Framework in Education Research Facilitates Civic Responsibility},
Journal = {EDUCATIONAL RESEARCHER},
