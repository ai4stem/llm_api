@article{ WOS:001326827700001,
Author = {Harada, Yuki and Hino, Sakura},
Title = {Correlation of Reaction Time Differences in Naïve vs. Scientific Concept
   Conflict Task with the Stroop Interference Effect: Exploring the
   Concepts that ``Moving Things are Alive{''} or ``Stationary Things are
   Not{''}},
Journal = {MIND BRAIN AND EDUCATION},
Year = {2024},
Volume = {18},
Number = {4},
Pages = {417-426},
Month = {NOV},
Abstract = {Recent studies have proposed that the automatic activation of na \&
   iuml;ve concepts, which persist even after learning scientific concepts,
   must be inhibited to successfully output scientific concepts. This model
   assumes that individual differences in inhibitory control (IC) within
   executive functions represent differences in conceptual change. However,
   the reported correlations between concept conflict and IC tasks are
   weak, casting doubt on this assumption. This study improves on previous
   methodologies by focusing on the performance of adult subjects in living
   and nonliving concept tasks presumed to have successfully achieved
   conceptual change and quantifying semantic-level IC through the Stroop
   interference effect. The results revealed that the correlations between
   the Stroop interference effect and reaction time (RT) differences
   between conditions of the concept conflict task, which are assumed to
   represent the time required to inhibit na \& iuml;ve concepts, were
   relatively stronger compared with findings reported in previous studies.
   These findings suggest that individual differences in fluent inhibition
   of na \& iuml;ve concepts after acquiring scientific concepts align with
   those in IC, thereby supporting the validity of the inhibition model of
   na \& iuml;ve concepts.
   Even after learning a scientific concept (e.g., plants are alive), a
   previously held na \& iuml;ve concept (e.g., moving objects are alive)
   remains in a person's brain. This study investigated the characteristics
   of individuals who can successfully and fluently ``suppress{''} na \&
   iuml;ve concepts in their brains when required to answer questions about
   scientific concepts. The results suggested a stronger connection than
   previously known with the well-known cognitive ability of inhibitory
   control.},
