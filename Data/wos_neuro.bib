@article{ WOS:000661805000001,
Author = {de Aguileta, Garazi Lopez and Soler Gallart, Marta},
Title = {Ausubel's Meaningful Learning and Educational Segregation},
Journal = {REMIE-MULTIDISCIPLINARY JOURNAL OF EDUCATIONAL RESEARCH},
