@article{ WOS:001110031200004,
Author = {Vizjak, Maja and Kaselj, Marina Peric and Paulisic, Morena},
Title = {Advantages of Digitization in Education - Knowledge Transfer as Modern
   Global Migration},
Journal = {CROATIAN JOURNAL OF EDUCATION-HRVATSKI CASOPIS ZA ODGOJ I OBRAZOVANJE},
Year = {2023},
