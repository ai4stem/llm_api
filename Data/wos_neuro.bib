@article{ WOS:000362054800003,
Author = {Smith, Richard and Keiner, Edwin},
Title = {Education and Science, Explaining and Understanding},
Journal = {ZEITSCHRIFT FUR PADAGOGIK},
Year = {2015},
Volume = {61},
