@article{ WOS:000728491400001,
Author = {Li, Wenjing and Wang, Fuxing and Mayer, Richard E. and Liu, Tao},
Title = {Animated pedagogical agents enhance learning outcomes and brain activity
   during learning},
Journal = {JOURNAL OF COMPUTER ASSISTED LEARNING},
Year = {2022},
