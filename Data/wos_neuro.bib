@article{ WOS:000332752000023,
Author = {Segedy, James R. and Biswas, Gautam and Sulcer, Brian},
Title = {A Model-Based Behavior Analysis Approach for Open-Ended Environments},
Journal = {EDUCATIONAL TECHNOLOGY \& SOCIETY},
