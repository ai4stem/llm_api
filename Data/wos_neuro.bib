@article{ WOS:000207809700001,
Author = {Hinton, Christina and Fischer, Kurt W.},
Title = {Research Schools: Grounding Research in Educational Practice},
Journal = {MIND BRAIN AND EDUCATION},
Year = {2008},
Volume = {2},
Number = {4},
Pages = {157-160},
Month = {DEC},
Abstract = {Education lacks a strong infrastructure for connecting research with
   educational practice and policy. The need for this linkage grows as
   findings in cognitive science and biology become ever more relevant to
   education. Teachers often lack the background knowledge needed to
   interpret scientific results, whereas scientists often lack an
   understanding of pedagogical goals. We need to build an infrastructure
   that supports sustainable collaboration between researchers and teachers
   and creates a strong research foundation for education. A primary agent
